<Window x:Class="BruteForceZipPasswordCracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BruteForceZipPasswordCracker"
        mc:Ignorable="d"
        Title="Zip Unlocker" Height="400" Width="600" Icon="/ZipUnlocker.jpg" MaxWidth="600" MaxHeight="400" MinWidth="600" MinHeight="400">
    <Grid Height="384.04" Width="600">
        <Label Content="ZipUnlocker" FontWeight="Bold" Margin="225,0,225,339" FontSize="24"/>
        <Label Content="Add file:" Margin="200,38,339,314"/>
        <TextBox Name="Input" Margin="200,60,200,305" AllowDrop="True" RenderTransformOrigin="0.437,-3.133"/>
        <Button Content="Unlock" Height="30" Width="90"  Click="Button_Click" Margin="255,35,255,214" />
        <ListBox d:ItemsSource="{d:SampleData ItemCount=10}" Margin="200,125,200,80"/>
        <Label Name="Pass" Visibility="Hidden" Content="Password is " FontWeight="DemiBold" Margin="230,310,295,50"/>
        <Label Name="Output" FontWeight="DemiBold"  Margin="295,310,230,50"/>

    </Grid>
</Window>
    
